<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>Search results - KITE Dashboard</title>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/pace.min.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/themes/black/pace-theme-minimal.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="assets/css/kite-dashboard.css" rel="stylesheet"/>

    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="https://rawgithub.com/olimsaidov/pixeden-stroke-7-icon/master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css" rel="stylesheet" />
    <style>
    a {
      color: black;
      text-decoration: none;
    }
    input, select, textarea{
        color: #000000;
        border-radius: 8px;
        padding: 2px 2px;
    }
    </style>
</head>
<body>
    <div class="wrapper">
        <!-- Start of side nav bar -->
        <div class="sidebar" data-color="green" data-image="assets/img/sidebarSmall.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="dashboard" class="simple-text">
                        KITE
                    </a>
                </div>

                <ul class="nav">
                    <li>
                        <div class="row">
                            <div class="col-sm-1 col-sm-offset-1">
                                <i class="pe-7s-search"></i>
                            </div>
                            <div class="col-sm-9 ">
                                <form action="search" method="get">
                                  <div>
                                    <input name="key" id="key" placeholder="Search">
                                  </div>
                                </form>
                            </div>

                        </div>
                    </li>
                    <li class="active">
                        <a href="dashboard">
                            <i class="pe-7s-note2"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="versions">
                            <i class="pe-7s-way"></i>
                            <p>Version Info</p>
                        </a>
                    </li>
                    <li class="">
                        <a href="config">
                            <i class="pe-7s-note"></i>
                            <p>Configurator</p>
                        </a>
                    </li>
                    <li>
                        <nav class="overview">
                            <ul>
                                <li><a href="#">
                                        <i class="pe-7s-pin"></i>
                                        <p>Overview</p>
                                    </a>
                                    <ul>
                                        #foreach($testName in $listOfTestOverview)
                                        <li><a href="overview?testname=$linkTool.encode($testName)" style="font-size: 12px;" >$testName<div class="circle"></div></a></li>
                                        #end
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End of side nav bar -->
        <!-- Start main data content display -->
        <div class="main-panel">
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            #set($listOfConfig = $result.getListOfConfigResult())
                            #set($listOfTest = $result.getListOfConfigTestResult())
                            #if ($listOfConfig.size() > 0)
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Config found:</h4>
                                    <p class="category">This list shows all configs found with the key word: $result.getSearchKey()</p>
                                </div>
                                <div class="content table-responsive table-full-width">
                                    <table class="table table-hover table-striped">
                                        <thead>
                                        <th>N.</th>
                                        <th>Config Name</th>
                                        <th>Start Time</th>
                                        <th>Status</th>
                                        </thead>
                                        <tbody>
                                            <!--Put velocity thing in here-->
                                            ## for each statement to iterate over a collection
                                            #set($index = 1)
                                            #foreach($config in $listOfConfig)
                                            <tr>
                                                <td><a href ="tests?id=$linkTool.encode($config.getConfigId())&configname=$linkTool.encode($config.getConfigName())">$index</a></td>
                                                <td><a href ="tests?id=$linkTool.encode($config.getConfigId())&configname=$linkTool.encode($config.getConfigName())">$config.getConfigName()</a></td>
                                                <td><a href ="tests?id=$linkTool.encode($config.getConfigId())&configname=$linkTool.encode($config.getConfigName())">$dateTool.format($config.getStartTime())</a></td>
                                                #if ($config.getIsDone())
                                                    <td><a href ="tests?id=$linkTool.encode($config.getConfigId())&configname=$linkTool.encode($config.getConfigName())">DONE</a></td>
                                                #else
                                                    <td><a href ="tests?id=$linkTool.encode($config.getConfigId())&configname=$linkTool.encode($config.getConfigName())">IN PROGRESS</a></td>
                                                #end
                                            </tr>
                                            #set($index = $index + 1)
                                            #end
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            #end
                            #if ($listOfTest.size() > 0)
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Tests found:</h4>
                                    <p class="category">This list shows all tests found with the key word: $result.getSearchKey()</p>
                                </div>
                                <div class="content table-responsive table-full-width">
                                    <table class="table table-hover table-striped">
                                        <thead>
                                        <th>N.</th>
                                        <th>Test Name</th>
                                        <th>Implementation Method</th>
                                        <th>Tuple Size</th>
                                        <th>Done/Total cases</th>
                                        <th>Status</th>
                                        <th>End Time</th>
                                        </thead>
                                        <tbody>
                                            #set($index = 1)
                                            #foreach($test in $listOfTest)
                                            <tr>
                                                <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$index</a></td>
                                                <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$test.getTestName()</a></td>
                                                <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$test.getImpl()</a></td>
                                                <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$test.getTupleSize()</a></td>
                                                <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$test.getDoneTests()/$test.getTotalTests()</a></td>
                                                #if ($test.getStatus())
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">Done</a></td>
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">$dateTool.format($test.getEndTime())</a></td>
                                                #elseif ($dateTool.getSystemTime() - $test.getStartTime() < 86400000)
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">In Progress</a></td>
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">NC</a></td>
                                                #else
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">Interrupted</a></td>
                                                    <td><a href ="results?name=$linkTool.encode($test.getResultTable())&testname=$linkTool.encode($test.getTestName())">NC</a></td>
                                                #end
                                            <tr>
                                            #set($index = $index + 1)
                                            #end
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            #end
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of main data content display -->
        </div>
    </div>
</body>
    <!--   Core JS Files   -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.js" type="text/javascript"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>

    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="assets/js/light-bootstrap-dashboard.js"></script>
</html>